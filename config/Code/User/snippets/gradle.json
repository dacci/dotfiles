{
  "Java Project": {
    "prefix": "java",
    "body": [
      "plugins {",
      "  id 'java'",
      "  id 'jacoco'",
      "  id 'application'",
      "  id 'com.diffplug.spotless' version '${2:5.11.0}'",
      "  id 'net.ltgt.errorprone' version '${3:1.3.0}'",
      "}",
      "",
      "sourceCompatibility = '11'",
      "",
      "configurations {",
      "  compileOnly {",
      "    extendsFrom annotationProcessor",
      "  }",
      "}",
      "",
      "repositories {",
      "  mavenCentral()",
      "}",
      "",
      "dependencies {",
      "  implementation platform('org.junit:junit-bom:${4:latest.release}')",
      "$0",
      "  compileOnly 'org.projectlombok:lombok:${5:latest.release}'",
      "  annotationProcessor 'org.projectlombok:lombok:${5:latest.release}'",
      "",
      "  implementation 'org.slf4j:slf4j-api:${6:latest.release}'",
      "  runtimeOnly('ch.qos.logback:logback-classic:${7:latest.release}') {",
      "    exclude group: 'com.sun.mail', module: 'javax.mail'",
      "  }",
      "",
      "  testImplementation 'org.junit.jupiter:junit-jupiter-api'",
      "  testRuntimeOnly 'org.junit.jupiter:junit-jupiter-engine'",
      "",
      "  errorprone 'com.google.errorprone:error_prone_core:latest.release'",
      "}",
      "",
      "spotless {",
      "  java {",
      "    googleJavaFormat()",
      "    importOrder 'java', 'javax', 'org', 'com', ''",
      "  }",
      "}",
      "",
      "compileJava.options.encoding = 'UTF-8'",
      "compileTestJava.options.encoding = 'UTF-8'",
      "",
      "test {",
      "  useJUnitPlatform()",
      "  finalizedBy jacocoTestReport",
      "}",
      "",
      "jacocoTestReport {",
      "  dependsOn test",
      "}",
      "",
      "application {",
      "  mainClass = '$1'",
      "}"
    ]
  },
  "AWS SDK": {
    "prefix": "aws",
    "body": "implementation platform('software.amazon.awssdk:bom:${1:latest.release}')$0"
  },
  "AWS SDK v1": {
    "prefix": "aws",
    "body": "implementation platform('com.amazonaws:aws-java-sdk-bom:${1:latest.release}')$0"
  }
}
